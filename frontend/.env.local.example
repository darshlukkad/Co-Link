# CoLink Frontend Environment Variables
# Copy this file to .env.local and fill in the values

# API Configuration
NEXT_PUBLIC_API_URL=http://localhost:8000
NEXT_PUBLIC_WS_URL=ws://localhost:8007

# Keycloak SSO Configuration
NEXT_PUBLIC_KEYCLOAK_URL=http://localhost:8080
NEXT_PUBLIC_KEYCLOAK_REALM=colink
NEXT_PUBLIC_KEYCLOAK_CLIENT_ID=colink-web

# Optional: Feature Flags
NEXT_PUBLIC_ENABLE_VIDEO_CALLS=false
NEXT_PUBLIC_ENABLE_VOICE_CALLS=false
NEXT_PUBLIC_ENABLE_FILE_PREVIEW=true

# Optional: Analytics
NEXT_PUBLIC_ANALYTICS_ID=

# Optional: Sentry Error Tracking
NEXT_PUBLIC_SENTRY_DSN=
